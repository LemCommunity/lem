{
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:import/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "root": true,
    "plugins": ["jsx-a11y", "@typescript-eslint"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "project": ["**/tsconfig.json"]
    }
  }
